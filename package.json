    "id": "cordova-plugin-admob-simple",
    "platforms": [
      "android",
      "ios",
      "wp8",
      "windows"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunnycupertino/cordova-plugin-admob-simple.git"
  },
  "bugs": {
    "url": "https://github.com/sunnycupertino/cordova-plugin-admob-simple/issues"
  },
  "keywords": [
    "cordova",
    "admob",
    "plugin",
    "phonegap",
    "intel-xdk",
    "intel",
    "xdk",
    "crosswalk",
    "ionic",
    "meteor",
    "ad",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "cordova-wp8",
    "google",
    "ads",
    "eclipse",
    "android-studio",
    "simple",
    "android",
    "ios",
    "windows",
    "phonegap",
    "monetization",
    "monetisation",
    "money",
    "banner",
    "interstitial",
    "advertisement",
    "advertising",
    "earn",
    "cordova-admob",
    "admobpro"
  ],
  "tonicExampleFilename": "test.js",
