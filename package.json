    "build": "babel src/js --out-dir www",
    "version": "sync-package-version --cordova-plugin",
    "test": "cordova-test"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "cordova-android": "^6.0.0",
    "cordova-test-cli": "0.0.1",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4",
    "sync-package-version": "0.0.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
